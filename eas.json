{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_DOMAIN": "YOUR_BACKEND_DOMAIN_HERE",
        "EXPO_PUBLIC_API_KEY": "YOUR_API_KEY_HERE"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_DOMAIN": "YOUR_BACKEND_DOMAIN_HERE",
        "EXPO_PUBLIC_API_KEY": "YOUR_API_KEY_HERE"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_DOMAIN": "YOUR_BACKEND_DOMAIN_HERE",
        "EXPO_PUBLIC_API_KEY": "YOUR_API_KEY_HERE"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
