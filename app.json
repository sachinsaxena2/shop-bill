{
  "expo": {
    "name": "Nazaara",
    "slug": "nazaara-billing",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nazaara",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nazaara.billing"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#006A6A",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.nazaara.billing",
      "versionCode": 5,
      "usesCleartextTraffic": true,
      "networkSecurityConfig": "./android-network-security-config.xml"
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#006A6A",
          "dark": {
            "backgroundColor": "#004D4D"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "EXPO_PUBLIC_API_KEY": "nazZEw5vhOhw1tLBGTwVhxs9MQrW2hjhY4h",
      "EXPO_PUBLIC_DOMAIN": "http://35.208.236.54",
      "eas": {
        "projectId": "a23aa57a-b097-4327-9bc1-763a5aad66c3"
      }
    }
  }
}
